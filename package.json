{
	"name": "@rose-pine/palette",
	"version": "4.0.1-1",
	"description": "Color palette tool for Rosé Pine",
	"license": "MIT",
	"repository": "rose-pine/palette",
	"funding": "https://github.com/rose-pine/palette?sponsor=1",
	"author": "Rosé Pine <hi@rosepinetheme.com>",
	"type": "module",
	"exports": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "del-cli dist && tsc && node lib/build.js && npm test",
		"test": "xo && ava",
		"release": "npx np@latest",
		"version": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"colors",
		"theme",
		"palette",
		"soho-vibes",
		"rose-pine"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@mvllow/tsconfig": "^0.2.2",
		"ava": "^5.2.0",
		"csso": "^5.0.5",
		"del-cli": "^5.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"xo": "^0.53.1"
	},
	"prettier": {
		"bracketSpacing": false,
		"semi": true,
		"singleQuote": false,
		"trailingComma": "es5",
		"useTabs": true
	},
	"xo": {
		"prettier": true,
		"ignores": [
			"dist/"
		]
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	}
}
