{
	"name": "@rose-pine/palette",
	"version": "2.0.0",
	"description": "Color palette tool for Rosé Pine",
	"license": "MIT",
	"repository": "rose-pine/palette",
	"funding": "https://github.com/rose-pine/palette?sponsor=1",
	"author": {
		"name": "Rosé Pine",
		"email": "hi@rosepinetheme.com"
	},
	"type": "module",
	"exports": "./index.js",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "npm run build:css && npm test",
		"build:css": "node lib/css/build.js",
		"test": "xo && ava",
		"release": "np",
		"prepare": "npm run build"
	},
	"files": [
		"css/",
		"index.js",
		"index.d.ts"
	],
	"types": "index.d.ts",
	"keywords": [
		"colors",
		"theme",
		"palette",
		"soho-vibes",
		"rose-pine"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"csso": "^5.0.2",
		"np": "^7.6.0",
		"param-case": "^3.0.4",
		"prettier": "^2.5.1",
		"xo": "^0.47.0"
	},
	"prettier": {
		"bracketSpacing": false,
		"semi": false,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	},
	"xo": {
		"prettier": true
	},
	"ava": {
		"nonSemVerExperiments": {
			"configurableModuleFormat": true
		}
	}
}
